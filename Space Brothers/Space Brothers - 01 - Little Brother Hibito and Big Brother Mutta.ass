Dialogue: 0,0:16:49.69,0:16:51.74,Default,Mutta,0000,0000,0000,,Zašto li želi da je sada preslušam?
Dialogue: 0,0:17:00.62,0:17:03.71,Default,Mutta,0000,0000,0000,,{\i1}Otkrio sam japansku šumsku žabu u šipražju iza naselja.{\i0}
Dialogue: 0,0:17:04.66,0:17:06.63,Default,Hibito,0000,0000,0000,,{\i1}Mu-čan!{\i0}
Dialogue: 0,0:17:07.00,0:17:08.92,Default,Hibito,0000,0000,0000,,{\i1}Mu-čan, ovamo! Brzo!{\i0}
Dialogue: 0,0:17:08.92,0:17:10.92,Default,Mutta,0000,0000,0000,,{\i1}Šta je bilo?{\i0}
Dialogue: 0,0:17:10.92,0:17:12.51,Default,Hibito,0000,0000,0000,,{\i1}Tako si spor...{\i0}
Dialogue: 0,0:17:12.51,0:17:13.01,Default,Mutta,0000,0000,0000,,Opa!
Dialogue: 0,0:17:13.01,0:17:14.17,Default,Mutta,0000,0000,0000,,Hej, Hibi... Hibito!
Dialogue: 0,0:17:14.17,0:17:15.68,Default,Mutta,0000,0000,0000,,Hibito! Prekini!
Dialogue: 0,0:17:19.05,0:17:20.60,Default,Mutta,0000,0000,0000,,Šta je to?
Dialogue: 0,0:17:20.60,0:17:21.60,Default,Hibito,0000,0000,0000,,Ne znam.
Dialogue: 0,0:17:21.60,0:17:23.35,Default,Mutta,0000,0000,0000,,{\i1}Ovo je trenutak kada se NLO pojavio.{\i0}
Dialogue: 0,0:17:24.81,0:17:26.56,Default,Mutta,0000,0000,0000,,{\i1}K-Kakve su ovo smetnje?{\i0}
Dialogue: 0,0:17:27.31,0:17:29.02,Default,Mutta,0000,0000,0000,,{\i1}Ne sećam se da sam ovo čuo...{\i0}
Dialogue: 0,0:17:31.40,0:17:32.36,Default,Mutta,0000,0000,0000,,{\i1}Ah...{\i0}
Dialogue: 0,0:17:33.23,0:17:34.69,Default,Mutta,0000,0000,0000,,{\i1}Ozbiljno?{\i0}
Dialogue: 0,0:17:44.91,0:17:47.50,Default,Mutta,0000,0000,0000,,{\i1}O-Otišlo je na Mesec?{\i0}
Dialogue: 0,0:17:47.50,0:17:50.21,Default,Hibito,0000,0000,0000,,{\i1}Mu-čan, ja mislim da...{\i0}
Dialogue: 0,0:17:50.21,0:17:53.05,Default,Hibito,0000,0000,0000,,Mislim da želim da postanem astronaut kada odrastem.
Dialogue: 0,0:17:53.67,0:17:55.59,Default,Hibito,0000,0000,0000,,Mislim da bih mogao da odem na Mesec.
Dialogue: 0,0:17:56.30,0:17:58.39,Default,Mutta,0000,0000,0000,,{\i1}P-Prvi je to rekao!{\i0}
Dialogue: 0,0:17:58.84,0:18:00.14,Default,Hibito,0000,0000,0000,,A šta ćeš ti da radiš, Mu-čan?
Dialogue: 0,0:18:00.43,0:18:02.35,Default,Mutta,0000,0000,0000,,Ha? Ja?
Dialogue: 0,0:18:03.01,0:18:05.31,Default,Mutta,0000,0000,0000,,J-Ja...
Dialogue: 0,0:18:06.27,0:18:07.98,Default,Mutta,0000,0000,0000,,Ja ću...
Dialogue: 0,0:18:07.98,0:18:09.90,Default,Mutta,0000,0000,0000,,{\i1}Šta li sam odgovorio?{\i0}
Dialogue: 0,0:18:13.36,0:18:15.69,Default,Mutta,0000,0000,0000,,Ako ćeš ti da ideš na Mesec,
Dialogue: 0,0:18:15.69,0:18:17.99,Default,Mutta,0000,0000,0000,,onda ću, naravno, ja otići korak dalje.
Dialogue: 0,0:18:20.37,0:18:21.07,Default,Mutta,0000,0000,0000,,Ja ću...
Dialogue: 0,0:18:24.79,0:18:26.33,Default,Mutta,0000,0000,0000,,otići na Mars.
Dialogue: 0,0:18:35.34,0:18:36.67,Default,Hibito,0000,0000,0000,,Onda je to obećanje.
Dialogue: 0,0:18:36.97,0:18:39.84,Default,Mutta,0000,0000,0000,,T-Tako je... obećanje.
Dialogue: 0,0:18:40.51,0:18:43.43,Default,Mutta,0000,0000,0000,,Nas dvojica ćemo postati astronauti.
Dialogue: 0,0:18:43.93,0:18:44.60,Default,Hibito,0000,0000,0000,,Aha.
Dialogue: 0,0:18:50.65,0:18:52.23,Default,Mutta,0000,0000,0000,,Obećanje...
Dialogue: 0,0:18:53.52,0:18:56.90,Default,Mutta,0000,0000,0000,,{\i1}Znači to je ono što je Hibito želeo da čujem.{\i0}
Dialogue: 0,0:19:03.91,0:19:07.20,Default,Mutta,0000,0000,0000,,{\i1}Da li mi to govoriš da treba da ispunim obećanje,{\i0}
Dialogue: 0,0:19:08.00,0:19:08.96,Default,Mutta,0000,0000,0000,,{\i1}Hibito?{\i0}
Dialogue: 0,0:19:11.12,0:19:14.63,dates,Caption,0000,0000,0000,,{\blur2\bord0.5\pos(640,572)}Mesec dana kasnije
Dialogue: 0,0:19:15.17,0:19:17.09,Default,A,0000,0000,0000,,D-Deset hiljada?!
Dialogue: 0,0:19:17.09,0:19:17.84,Default,A,0000,0000,0000,,Ne zavitlavajte me!
Dialogue: 0,0:19:17.84,0:19:20.51,Default,A,0000,0000,0000,,Samo sam otišao na deset minuta da kupim ručak!
Dialogue: 0,0:19:20.51,0:19:23.64,Default,A,0000,0000,0000,,Kako je kazna za to deset hiljada jena?
Dialogue: 0,0:19:23.64,0:19:25.14,Default,A,0000,0000,0000,,Nemoj samo da stojiš i da ćutiš tu!
Dialogue: 0,0:19:25.14,0:19:25.76,Default,A,0000,0000,0000,,Reci nešto, čupavi!
Dialogue: 0,0:19:25.76,0:19:28.39,Default,Mutta,0000,0000,0000,,Uh, u stvari, petnaest hiljada je.
Dialogue: 0,0:19:28.39,0:19:29.43,Default,A,0000,0000,0000,,Ćuti! Tišina!
Dialogue: 0,0:19:29.43,0:19:31.52,Default,A,0000,0000,0000,,Neću da platim ni deset ni petnaest hiljada!
Dialogue: 0,0:19:31.52,0:19:33.65,Default,A,0000,0000,0000,,Nemam ja te pare!
Dialogue: 0,0:19:33.65,0:19:37.90,Default,A,0000,0000,0000,,A i pogledajte koliko mesta ima, nikoga ne blokiram!
Dialogue: 0,0:19:35.82,0:19:36.98,Default,Mutta,0000,0000,0000,,{\an8\i1}Kakva ironija.{\i0}
Dialogue: 0,0:19:37.90,0:19:40.07,Default,Mutta,0000,0000,0000,,{\an8\i1}I ovaj auto sam ja dizajnirao.{\i0}
Dialogue: 0,0:19:37.90,0:19:41.03,Default,A,0000,0000,0000,,Ovaj čupavi verovatno pravi više problema u saobraćaju nego ja!
Dialogue: 0,0:19:40.40,0:19:41.82,Default,Mutta,0000,0000,0000,,{\an8\i1}Kako tužno...{\i0}
Dialogue: 0,0:19:41.82,0:19:43.11,Default,A,0000,0000,0000,,Slušaš li me, čupavi?
Dialogue: 0,0:19:43.11,0:19:44.20,Default,A,0000,0000,0000,,Hoćeš li reći nešto?
Dialogue: 0,0:19:44.20,0:19:46.16,Default,Mutta,0000,0000,0000,,T-To će biti petnaest hiljada jena.
Dialogue: 0,0:19:46.16,0:19:47.41,Default,A,0000,0000,0000,,Tišina! Ćuti! Ćuti!
Dialogue: 0,0:19:47.41,0:19:49.79,Default,A,0000,0000,0000,,Vaša računica nema smisla!
Dialogue: 0,0:19:48.45,0:19:52.17,Default,Mutta,0000,0000,0000,,{\an8\i1}Čoveče, moraću da potražim drugi posao...{\i0}
Dialogue: 0,0:19:49.79,0:19:52.17,Default,A,0000,0000,0000,,Znaš li ti koliko ručaka mogu da kupim sa 15.000 jena?
Dialogue: 0,0:19:53.17,0:19:56.21,Default,Mutta,0000,0000,0000,,{\i1}Želim što pre da se vratim kući da dovršim knjigu koju sam započeo.{\i0}
Dialogue: 0,0:19:56.88,0:19:59.34,Default,Mutta,0000,0000,0000,,{\i1}I iako sam znao da je to nedostižan san...{\i0}
Dialogue: 0,0:20:00.38,0:20:01.59,Default,Mutta,0000,0000,0000,,{\i1}Čoveče...{\i0}
Dialogue: 0,0:20:02.18,0:20:06.55,Default,Mutta,0000,0000,0000,,{\i1}Želim da sanjarim o svemiru kao nekada što sam činio.{\i0}
Dialogue: 0,0:20:09.31,0:20:12.06,Default,Mutta,0000,0000,0000,,{\i1}I na kraju sam opet dao otkaz.{\i0}
Dialogue: 0,0:20:19.19,0:20:21.28,Default,A,0000,0000,0000,,Požuri! Hajde, požuri!
Dialogue: 0,0:20:21.78,0:20:23.24,Default,B,0000,0000,0000,,Čekaj, bato...
Dialogue: 0,0:20:23.70,0:20:25.12,Default,A,0000,0000,0000,,Požuri!
Dialogue: 0,0:20:25.12,0:20:27.03,Default,B,0000,0000,0000,,Bato!
Dialogue: 0,0:20:27.03,0:20:30.91,Default,Mutta,0000,0000,0000,,{\i1}Stariji brat uvek mora da ostane nekoliko koraka ispred mlađeg,{\i0}
Dialogue: 0,0:20:31.83,0:20:35.88,Default,Mutta,0000,0000,0000,,{\i1}ali ja ne mogu čak ni taj jedan korak da napravim!{\i0}
Dialogue: 0,0:20:36.46,0:20:37.42,Default,Mutta,0000,0000,0000,,{\i1}Hibito...{\i0}
Dialogue: 0,0:20:37.92,0:20:40.30,Default,Mutta,0000,0000,0000,,{\i1}Izvini, ali možeš li da zaboraviš na ono obećanje?{\i0}
Dialogue: 0,0:20:41.13,0:20:42.42,Default,Mutta,0000,0000,0000,,Vratio sam se...
Dialogue: 0,0:20:42.42,0:20:43.80,Default,Mom,0000,0000,0000,,Dobro došao nazad.
