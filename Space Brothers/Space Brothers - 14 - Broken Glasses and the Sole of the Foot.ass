Dialogue: 0,0:00:01.68,0:00:04.65,Default,Mu,0000,0000,0000,,{\i1}Prošlo je pet dana otkako smo počeli da živimo
Dialogue: 0,0:00:04.65,0:00:08.40,Default,Mu,0000,0000,0000,,{\i1}unutar izolacionih blokova koji nas odsecaju od ostatka sveta.
Dialogue: 0,0:00:08.78,0:00:13.32,Default,Mu,0000,0000,0000,,{\i1}Nisam mislio da će nas nadgledati čak i tokom slobodnog vremena.
Dialogue: 0,0:00:14.20,0:00:16.20,Default,Both,0000,0000,0000,,Iskezi se i ostani tako...
Dialogue: 0,0:00:16.20,0:00:19.79,Default,Both,0000,0000,0000,,Ako se nasmeješ, izgubio si!
Dialogue: 0,0:00:41.89,0:00:43.60,Default,Furu,0000,0000,0000,,To! Ovo je druga pobeda!
Dialogue: 0,0:00:44.52,0:00:46.15,Default,Mu,0000,0000,0000,,{\i1}Kakvo smešno lice.
Dialogue: 0,0:00:46.48,0:00:48.48,Default,Furu,0000,0000,0000,,Hoćeš i ti da probaš, Nita?
Dialogue: 0,0:00:48.86,0:00:50.28,Default,Furu,0000,0000,0000,,Ostaćeš bez pudinga.
Dialogue: 0,0:00:52.61,0:00:53.95,Default,Ni,0000,0000,0000,,Možeš da uzmeš moj.
Dialogue: 0,0:00:58.70,0:01:00.37,Default,Fuku,0000,0000,0000,,Onda sam ja poslednji.
Dialogue: 0,0:01:02.41,0:01:04.46,Default,Furu,0000,0000,0000,,Dobro, hajdemo onda.
Dialogue: 0,0:01:04.83,0:01:06.29,Default,Furu,0000,0000,0000,,Spreman?
Dialogue: 0,0:01:18.85,0:01:19.97,Default,Mu,0000,0000,0000,,Fukuda...
Comment: 0,0:01:32.19,0:01:32.78,Default,Furu,0000,0000,0000,,Crap!
Dialogue: 0,0:01:33.19,0:01:37.16,Default,Furu,0000,0000,0000,,Šta je ovo, ćale, nisam znao da imaš tako smrtonosno oružje...
Dialogue: 0,0:01:37.16,0:01:39.49,Default,Fuku,0000,0000,0000,,Uvek sam bio dobar u ovoj igri.
Dialogue: 0,0:01:39.49,0:01:40.99,Default,Fuku,0000,0000,0000,,Uzimam ti puding.
Dialogue: 0,0:01:40.99,0:01:43.12,Default,Seri,0000,0000,0000,,To je bilo neverovatno, Fukuda...
Dialogue: 0,0:01:43.37,0:01:46.58,Default,Furu,0000,0000,0000,,Lice ti je baš rastegljivo!
Dialogue: 0,0:01:47.96,0:01:49.08,Default,Mu,0000,0000,0000,,{\i1}Fukuda Naoto,
Dialogue: 0,0:01:49.67,0:01:51.67,Default,Mu,0000,0000,0000,,{\i1}zastrašujući 54-godišnjak.
Dialogue: 0,0:01:54.01,0:01:54.05,Default,,0000,0000,0000,,{OP}
Dialogue: 0,0:01:54.01,0:01:55.63,Pesmice,,0000,0000,0000,,Podići ću se,
Dialogue: 0,0:01:55.63,0:01:57.18,Pesmice,,0000,0000,0000,,penjati,
Dialogue: 0,0:01:57.18,0:01:59.84,Pesmice,,0000,0000,0000,,i probiti kroz nebesa.
Dialogue: 0,0:02:01.01,0:02:05.39,Pesmice,,0000,0000,0000,,Moj cilj je nulta gravitacija.
