Dialogue: 0,0:02:19.41,0:02:22.58,Pesmice,,0000,0000,0000,,Talasi ljudi me zapljuskuju na raskrsnici,
Dialogue: 0,0:02:22.58,0:02:25.66,Pesmice,,0000,0000,0000,,kao blede senke promiču pored mene ne zastajući.
Dialogue: 0,0:02:25.66,0:02:29.04,Pesmice,,0000,0000,0000,,Kako se njima čine moji obrisi?
Dialogue: 0,0:02:29.04,0:02:31.13,Pesmice,,0000,0000,0000,,Ne znam.
Dialogue: 0,0:02:32.17,0:02:35.46,Pesmice,,0000,0000,0000,,Očajnički krijući svoje stvarne misli
Dialogue: 0,0:02:35.46,0:02:38.68,Pesmice,,0000,0000,0000,,kao izgovor dajem prvo što mi padne na pamet.
Dialogue: 0,0:02:38.68,0:02:41.89,Pesmice,,0000,0000,0000,,Moje strasti ka svemiru koje sam tako dugo čuvao
Dialogue: 0,0:02:41.89,0:02:44.76,Pesmice,,0000,0000,0000,,su male i krhke.
Dialogue: 0,0:02:45.10,0:02:48.39,Pesmice,,0000,0000,0000,,Kako god zavrtim globus
Dialogue: 0,0:02:48.64,0:02:51.69,Pesmice,,0000,0000,0000,,nema pravog mesta za mene.
Dialogue: 0,0:02:52.06,0:02:57.57,Pesmice,,0000,0000,0000,,Nema svrhe gledati kartu sveta.
Dialogue: 0,0:02:58.44,0:03:00.07,Pesmice,,0000,0000,0000,,Podići ću se,
Dialogue: 0,0:03:00.07,0:03:01.66,Pesmice,,0000,0000,0000,,penjati,
Dialogue: 0,0:03:01.66,0:03:04.24,Pesmice,,0000,0000,0000,,i probiti kroz nebesa
Dialogue: 0,0:03:04.78,0:03:10.79,Pesmice,,0000,0000,0000,,dok mi svet ne bude pod nogama.
Dialogue: 0,0:03:11.25,0:03:12.96,Pesmice,,0000,0000,0000,,Prodraću se
Dialogue: 0,0:03:12.96,0:03:14.38,Pesmice,,0000,0000,0000,,i vikati
Dialogue: 0,0:03:14.38,0:03:16.55,Pesmice,,0000,0000,0000,,iz srca očeličenog
Dialogue: 0,0:03:18.26,0:03:23.30,Pesmice,,0000,0000,0000,,{\fad(0,347)}da je moj cilj nulta gravitacija.
Dialogue: 0,0:03:23.97,0:03:24.01,Default,,0000,0000,0000,,{part a}
