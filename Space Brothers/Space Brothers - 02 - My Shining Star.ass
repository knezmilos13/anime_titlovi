Dialogue: 0,0:12:03.07,0:12:05.16,Default,Mutta,0000,0000,0000,,{\i1}Daj bože da prođem u drugi krug!{\i0}
Dialogue: 0,0:12:07.03,0:12:08.91,Default,Mutta,0000,0000,0000,,{\i1}Želim da idem u svemir.{\i0}
Dialogue: 0,0:12:09.49,0:12:12.16,Default,Mutta,0000,0000,0000,,{\i1}To je sve što me pokreće sada.{\i0}
Dialogue: 0,0:12:18.80,0:12:21.72,Typesetting,Caption,0000,0000,0000,,{\fad(0,450)\fscx90\fs50\blur0.4\fnArial\b1\c&HFFFFFF&\pos(655,253)}3 NEDELJE KASNIJE
Dialogue: 0,0:12:22.80,0:12:26.22,Default,A,0000,0000,0000,,{\i1}I evo ga! Govedina čini čak 72,5% smeše...{\i0}
Dialogue: 0,0:12:26.22,0:12:28.60,Default,A,0000,0000,0000,,{\i1}Novi bon kari.{\i0}
Dialogue: 0,0:12:28.89,0:12:30.52,Default,B,0000,0000,0000,,{\i1}Daj sve od sebe, Masao.{\i0}
Dialogue: 0,0:12:31.27,0:12:32.31,Default,Mutta,0000,0000,0000,,Vratio sam se.
Dialogue: 0,0:12:32.64,0:12:34.06,Default,Mom,0000,0000,0000,,Dobro došao nazad!
Dialogue: 0,0:12:34.06,0:12:35.35,Default,Mutta,0000,0000,0000,,Šta je za večeru?
Dialogue: 0,0:12:35.35,0:12:36.27,Default,Mom,0000,0000,0000,,Bon kari.
Dialogue: 0,0:12:36.56,0:12:38.65,Default,Mutta,0000,0000,0000,,Ah, zeznuo sam!
Dialogue: 0,0:12:38.65,0:12:40.82,Default,Mutta,0000,0000,0000,,I za ručak sam jeo kari!
Dialogue: 0,0:12:40.82,0:12:43.60,Default,Mom,0000,0000,0000,,Ne brini... Ovo je bon kari udon.
Dialogue: 0,0:12:43.90,0:12:45.61,Default,Mom,0000,0000,0000,,A, da, Mu-čan.
Dialogue: 0,0:12:45.61,0:12:47.66,Default,Mom,0000,0000,0000,,Stiglo ti je nešto iz Džakse.
Dialogue: 0,0:12:47.66,0:12:49.53,Default,Mom,0000,0000,0000,,Možda ti javljaju da si položio?
Dialogue: 0,0:12:52.20,0:12:53.58,Default,Mutta,0000,0000,0000,,Mama!
Dialogue: 0,0:12:53.58,0:12:56.38,Default,Mutta,0000,0000,0000,,Ovo je trebalo da pomeneš pre bon karija!
Dialogue: 0,0:13:10.10,0:13:11.26,Default,Mom,0000,0000,0000,,Pa?
Dialogue: 0,0:13:15.27,0:13:16.02,Default,Mom,0000,0000,0000,,Ah!
Dialogue: 0,0:13:20.48,0:13:23.07,Default,Mom,0000,0000,0000,,Hej! Gde si krenuo, Muta?
Dialogue: 0,0:13:23.40,0:13:26.03,Default,Mom,0000,0000,0000,,Kupi mleko na povratku!
Dialogue: 0,0:13:26.03,0:13:27.78,Default,Mom,0000,0000,0000,,Obrano mleko!
Dialogue: 0,0:13:30.53,0:13:31.74,Default,Mutta,0000,0000,0000,,{\i1}Neverovatno.{\i0}
Dialogue: 0,0:13:31.74,0:13:32.74,Default,Mutta,0000,0000,0000,,{\i1}Neverovatno...{\i0}
Dialogue: 0,0:13:33.37,0:13:35.08,Default,Mutta,0000,0000,0000,,{\i1}Neverovatno!{\i0}
Dialogue: 0,0:13:46.17,0:13:47.84,Default,A,0000,0000,0000,,Čekaj me!
Dialogue: 0,0:13:52.56,0:13:53.64,Default,B,0000,0000,0000,,Ne plači.
Dialogue: 0,0:13:54.52,0:13:56.98,Default,B,0000,0000,0000,,Hej, prekini da plačeš!
Dialogue: 0,0:14:05.44,0:14:07.57,Default,Mom,0000,0000,0000,,Ah, moraću da kupim tortu!
Dialogue: 0,0:14:10.49,0:14:11.49,Default,A,0000,0000,0000,,Koja je to pesma?
Dialogue: 0,0:14:11.49,0:14:12.41,Default,B,0000,0000,0000,,Nemam pojma.
Dialogue: 0,0:14:12.41,0:14:14.45,Default,B,0000,0000,0000,,Ustani! Hajde!
Dialogue: 0,0:14:14.87,0:14:15.95,Default,B,0000,0000,0000,,Idemo.
Dialogue: 0,0:14:17.33,0:14:18.17,Default,B,0000,0000,0000,,Hajde!
Dialogue: 0,0:14:19.00,0:14:21.46,Default,Mutta,0000,0000,0000,,{\i1}Sviraću pesmu bez melodije,{\i0}
Dialogue: 0,0:14:22.04,0:14:24.21,Default,Mutta,0000,0000,0000,,{\i1}i ići ću tamo gde nema puta.{\i0}
Dialogue: 0,0:14:29.97,0:14:32.64,Default,Mutta,0000,0000,0000,,{\i1}To će me odvesti do moje{\i0}
Dialogue: 0,0:14:34.64,0:14:36.43,Default,Mutta,0000,0000,0000,,{\i1}svetlucave zvezde!{\i0}
Dialogue: 0,0:14:45.86,0:14:48.03,Default,Mutta,0000,0000,0000,,Da, bre, u redu je...
Dialogue: 0,0:14:48.57,0:14:50.53,Default,Mutta,0000,0000,0000,,Stigao sam u Džaksu!
Dialogue: 0,0:14:51.66,0:14:53.37,Default,Mutta,0000,0000,0000,,Pa, trenutno sam u toaletu.
Dialogue: 0,0:14:53.37,0:14:55.29,Default,Mom,0000,0000,0000,,{\i1}Još malo pa će drugi ispit!{\i0}
Dialogue: 0,0:14:55.29,0:14:57.54,Default,Mom,0000,0000,0000,,{\i1}Jesi li nervozan? Reci, jesi li nervozan?{\i0}
Dialogue: 0,0:14:57.54,0:15:00.71,Default,Mutta,0000,0000,0000,,Naravno da jesam... nemoj da me zadirkuješ, mama!
Dialogue: 0,0:15:00.71,0:15:03.38,Default,Mom,0000,0000,0000,,{\i1}Naučiću te trik kako da se smiriš u svakoj situaciji.{\i0}
Dialogue: 0,0:15:03.38,0:15:04.88,Default,Mutta,0000,0000,0000,,M-M-Ma u redu je...
Dialogue: 0,0:15:05.17,0:15:06.30,Default,Mutta,0000,0000,0000,,Hvala ti, mama.
Dialogue: 0,0:15:06.30,0:15:09.42,Default,Mom,0000,0000,0000,,{\i1}Treba prstom na dlanu da napišeš znak za čoveka stotinu puta.{\i0}
Dialogue: 0,0:15:09.42,0:15:10.72,Default,Mom,0000,0000,0000,,{\i1}To će ti zagolicati dlan...{\i0}
Dialogue: 0,0:15:11.68,0:15:13.39,Default,Mutta,0000,0000,0000,,M-Mama!
Dialogue: 0,0:15:23.65,0:15:27.90,Default,Mutta,0000,0000,0000,,{\i1}Ne, u ovakvom trenutku moram da razmišljam pozitivno.{\i0}
Dialogue: 0,0:15:29.24,0:15:31.86,Default,Mutta,0000,0000,0000,,{\i1}Više nemam šta da izgubim!{\i0}
Dialogue: 0,0:15:38.91,0:15:43.08,Default,Mutta,0000,0000,0000,,{\i1}Uključujući i mene, preostalo je još četrdeset i pet kandidata.{\i0}
